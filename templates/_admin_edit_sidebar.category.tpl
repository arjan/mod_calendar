        <span class="title">ICal feed</span>
        <span class="arrow">make smaller</span>
    </h3>
    <div class="item clearfix">
        <div class="form ">
            <div class="form-item clearfix">
                <label>Title<br />
                    <input type="text" id="twitter-id" name="ical_feed_title" class="zp-100" value="{{ m.rsc[id].ical_feed_title }}" />
                </label>
            </div>

            <div class="form-item clearfix">
                <label>Description<br />
                    <input type="text" id="twitter-id" name="ical_feed_desc" class="zp-100" value="{{ m.rsc[id].ical_feed_desc }}" />
                </label>
            </div>

        </div>
    </div>
</div>
